{
  "expo": {
    "name": "Guitar Chords Generator",
    "slug": "guitar-chords-generator",
    "version": "1.0.0",
    "platforms": ["android"],
    "orientation": "portrait",
    "icon": "./icon.png",
    "splash": {
      "image": "./assets/A_major.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.guitar-chord-generator",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "permissions": []
    },
    "web": {
      "favicon": "./assets/F_major.png"
    },
    "plugins": ["expo-av"]
  }
}
